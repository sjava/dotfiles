{
  "languageserver": {
    "elixirLS": {
      "command": "/home/zyb/tools/elixir-ls/dist/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "settings": {
        "elixirLS": {
          "dialyzerEnabled": false
        }
      }
    },
    "MPLS":{
      "command": "dotnet",
      "args": ["exec", "/usr/lib/microsoft-python-language-server/Microsoft.Python.LanguageServer.dll"],
      "filetypes": ["python"],
      "initializationOptions": {
        "interpreter": {
          "properties": {
            "InterpreterPath": "/home/zyb/.pyenv/versions/myapp/bin/python3",
            "UseDefaultDatabase": true,
            "Version": "3.6.4"
          }
        }
      }
    },
  }
}
